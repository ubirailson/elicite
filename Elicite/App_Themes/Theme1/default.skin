<asp:GridView runat="server" AutoGenerateColumns="False" CssClass="gridview" AllowPaging="True" 
    GridLines="Both">
    <FooterStyle BackColor="#0099ff" ForeColor="White" Font-Bold="true" HorizontalAlign="Right" />
    <PagerStyle CssClass="gridview_pager" HorizontalAlign="Center" />
    <SelectedRowStyle CssClass="gridview_selected_row" />
    <HeaderStyle CssClass="gridview_header" />
    <AlternatingRowStyle CssClass="gridview_alternating" />
</asp:GridView>
<%-- 
Skin para Menu
--%>
<asp:Menu runat="server" Orientation="Horizontal" BackColor="#0099ff" DynamicHorizontalOffset="10"
    DynamicVerticalOffset="8" ForeColor="White"  StaticSubMenuIndent="10px">
    <StaticMenuItemStyle HorizontalPadding="5px" VerticalPadding="2px" />
    <StaticSelectedStyle BackColor="#0099ff" />
    <StaticHoverStyle BackColor="#0099ff" ForeColor="White" />
    <DynamicHoverStyle BackColor="#0099ff" ForeColor="White" />
    <DynamicMenuStyle BackColor="#0099ff" />
    <DynamicSelectedStyle BackColor="#0099ff" />
    <DynamicMenuItemStyle HorizontalPadding="5px" VerticalPadding="2px" BorderColor="Silver" BorderWidth="1px" />
</asp:Menu>